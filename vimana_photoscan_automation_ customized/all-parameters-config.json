{
	"initial_setup":{
		"project_name":"The-columns"
	},
	"add_photos":{
	   "layout":"FlatLayout"
	},
	"estimate_image_quality":{
	   "threshold":0.5
	},
	"align_photos":{
	   "coordinate_system":"EPSG::4326",
	   "accuracy":"HighAccuracy",
	   "filter_mask":false,
	   "keypoint_limit":50000,
	   "preselection":"ReferencePreselection",
	   "generic_preselection":false,
	   "reference_preselection":true,
	   "tiepoint_limit":10000,
	   "fit_shutter":true,
	   "fit_p4":true,
	   "fit_p3":true,
	   "fit_p2":true,
	   "fit_p1":true,
	   "fit_k4":true,
	   "fit_k3":true,
	   "fit_k2":true,
	   "fit_k1":true,
	   "fit_b2":true,
	   "fit_b1":true,
	   "fit_cy":true,
	   "fit_cx":true,
	   "fit_f":true,
	   "adaptive_fitting":true,
	   "minimum_number_of_point_projection":1,
	   "exports":{
		  "export_cameras":{
			 "format":"CamerasFormatXML",
			 "coordinate_system":"EPSG::4326",
			 "rotation_order":"RotationOrderXYZ"
		  },
		  "export_matches":{
			 "format":"MatchesFormatBINGO",
			 "precision":3,
			 "port_markers":false,
			 "use_labels":false,
			 "export_points":true,
			 "export_markers":true
		  }
	   }
	},
	"add_gcps":{
	   "coordinate_system":"EPSG::9001"
	},
	"detect_markers":{
	   "type":"CircularTarget12bit",
	   "tolerance":100,
	   "inverted":false,
	   "noparity":false,
	   "exports":{
		  "export_markers":{
			 "coordinate_system":"EPSG::9001"
		  }
	   }
	},
	"build_dense_cloud":{
	   "keep_depth":false,
	   "filter":"MildFiltering",
	   "reuse_depth":false,
	   "quality":"HighQuality",
	   "exports":{
		  "export_points":{
			 "source":"DenseCloudData",
			 "binary":true,
			 "precision":6,
			 "normals":true,
			 "colors":true,
			 "format":"PointsFormatXYZ"
		  }
	   }
	},
	"build_mesh":{
	   "interpolation":"EnabledInterpolation",
	   "surface":"Arbitrary",
	   "source":"DenseCloudData",
	   "face_count":"HighFaceCount",
	   "vertex_colors":true,
	   "exports":{
		  "export_model":{
			 "binary":true,
			 "precision":6,
			 "texture_format":"ImageFormatJPEG",
			 "texture":true,
			 "normals":true,
			 "colors":true,
			 "cameras":true,
			 "markers":true,
			 "udim":false,
			 "strip_extensions":false
		  }
	   }
	},
	"merge_chunk":{
	   "merge_dense_clouds":true,
	   "merge_models":true,
	   "merge_markers":true
	},
	"build_texture":{
	   "blending":"MosaicBlending",
	   "fill_holes":true,
	   "texture_size":8192,
	   "color_correction":true,
	   "mapping":"OrthophotoMapping",
	   "texture_count":1
	},
	"build_dem":{
	   "coordinate_system":"EPSG::9001",
	   "interpolation":"EnabledInterpolation",
	   "source":"DenseCloudData",
	   "minimum_number_of_point_projection":1,
	   "reprojection_error_threshold":10,
	   "exports":{
		  "export_dem":{
			 "format":"PhotoScan.RasterFormatTiles",
			 "image_format":"PhotoScan.ImageFormatTIFF",
			 "raster_transform":"RasterTransformNone",
			 "projection":"EPSG::9001",
			 "nodata":-32767,
			 "write_kml":false,
			 "write_world":false,
			 "write_scheme":false,
			 "tiff_big":true
		  }
	   }
	},
	"build_orthomosaic":{
	   "fill_holes":true,
	   "blending":"MosaicBlending",
	   "surface":"ElevationData",
	   "color_correction":true,
	   "exports":{
		  "export_orthomosaic":{
			 "format":"PhotoScan.RasterFormatTiles",
			 "image_format":"PhotoScan.ImageFormatTIFF",
			 "raster_transform":"PhotoScan.RasterTransformNone",
			 "write_kml":false,
			 "write_world":false,
			 "write_alpha":true,
			 "tiff_compression":"PhotoScan.TiffCompressionNone",
			 "tiff_big":false,
			 "jpeg_quality":90,
			 "white_background":true
		  }
	   }
	}
 }